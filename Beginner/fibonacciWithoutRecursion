#include <iostream>

using namespace std;

int main()
{
    int n;
    cout<<"Enter length of fibonacci series : ";
    cin>>n;
    cout<<"\nSeries is : ";
    int a=0, b=1, x;
    if(n == 1){
        cout<<a;
        return 0;
    }
    if(n == 2){
        cout<<a<<" "<<b;
        return 0;
    }
    cout<<a<<" "<<b<<" ";
    for(int i=2; i<n; i++){
        x = a+b;
        cout<<x<<" ";
        a = b;
        b = x;
        
    }
    

    return 0;
}
